{
  "name": "root",
  "devDependencies": {
    "lerna": "^3.20.2",
    "minimist": ">=1.2.2",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2"
  },
  "scripts": {
    "audit": "lerna exec npm audit",
    "bootstrap": "lerna bootstrap",
    "clean": "lerna clean --yes",
    "install:package-locks": "lerna exec npm i --package-lock-only",
    "link": "lerna link",
    "start": "run-s bootstrap start:lerna",
    "start:lerna": "lerna run start --parallel",
    "test": "lerna run test",
    "build:watch": "lerna run build:watch",
    "update": "lerna exec -- npx npm-check-updates -u && lerna bootstrap"
  },
  "dependencies": {
    "handlebars": "^4.7.3",
    "npm-check-updates": "^4.0.4"
  }
}
